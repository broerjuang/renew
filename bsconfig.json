{
  "name": "renew",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "bs-dependencies": ["reason-react", "reason-react-native"],
  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "ReactNative": "ReactNative"
    },
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "reason": { "react-jsx": 3 },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bsc-flags": ["-bs-super-errors"],
  "refmt": 3
}
